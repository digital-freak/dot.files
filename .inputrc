set meta-flag on
set convert-meta off
set input-meta on
set output-meta on

# allow the use of the Home/End keys
"\e[1~": beginning-of-line
"\e[4~": end-of-line
"\e[7~": beginning-of-line
"\e[8~": end-of-line

# allow the use of the Delete/Insert keys
"\e[3~": delete-char
"\e[2~": quoted-insert

# allow the use of the Delete/Insert keys
"\e[3~": delete-char

$if term=cons25r
  "\C-?": delete-char
$endif

"\e[2~": quoted-insert


"\e[A": history-search-backward
"\e[B": history-search-forward

